  "main": "server.js",
  "scripts": {
    "dev": "npm run demigrate && npm run migrate && npm run seed:all && npx tsc && nodemon ./src/server.ts",
    "start": "npx tsc && cp src/swagger.json dist/swagger.json && node ./dist/server.js",
    "lint": "eslint . --ext .js,.ts --fix",
    "format": "prettier --write .",
    "prepare": "husky install",
    "test-lint": "npm run lint && npm run format",
    "test": "npm run demigrate && npm run migrate && npm run seed:all && jest --coverage --runInBand  --detectOpenHandles",
    "demigrate": "npx sequelize db:migrate:undo:all",
    "migrate": "npx sequelize db:migrate",
    "seed:all": "npx sequelize db:seed:all",
    "build": "tsc && cp src/swagger.json dist/swagger.json"
  },
  "lint-staged": {
    "src/**/*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
